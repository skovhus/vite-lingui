{
  "name": "vite-lingui",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "i18n": "yarn i18n:extract && yarn i18n:compile",
    "i18n:extract": "lingui extract",
